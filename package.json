{
	"name": "Lombard24",
	"version": "0.0.19",
	"description": "Lombard24",
	"keywords": [
		"react",
		"typescript",
		"webpack",
		"storybook",
		"jest"
	],
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/SSazontsev/pawnshop-24"
	},
	"bugs": {
		"url": "https://github.com/SSazontsev/pawnshop-24/issues"
	},
	"homepage": "https://lombard24achinsk.netlify.app",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --env env=prod",
		"start": "webpack serve --config webpack/webpack.config.js --env env=dev",
		"preview": "serve dist",
		"stylelint": "stylelint \"src/**/*.scss\"",
		"stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
		"lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
		"format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path ./.prettierignore",
		"test:e2e": "cypress open",
		"ci-test:e2e": "cypress run",
		"test": "jest --verbose",
		"test:update": "jest --verbose -u",
		"test:watch": "jest --watchAll",
		"test:coverage": "jest --coverage",
		"prepare": "husky install",
		"prebuild": "npm run lint",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"commit": "npm run test:update && git add -A && cz && npx standard-version",
		"push": "git push origin ",
		"subset-fonts": "node subset-fonts.js",
		"clean": "rimraf dist",
		"deploy": "gh-pages -d dist"
	},
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
